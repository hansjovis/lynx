<!DOCTYPE html>
<html>
<head>
    <!-- Meta information -->
    <meta charset="utf-8">
    <title>{{title}}</title>
    <!-- Linked Data -->
    <script id="data" type="application/json+ld">{{{data}}}</script>
    <!-- Scripts -->
    {{#each scripts}}
    <script src="{{this}}" type="application/javascript"></script>
    {{/each}}
    <!-- Styles -->
    <link rel="stylesheet" href="css/main.css" type="text/css"/>
    {{#each styles}}
    <link rel="stylesheet" href="{{this}}" type="text/css"/>
    {{/each}}
</head>
<body>
    <main id="app">{{{body}}}</main>
</body>
</html>